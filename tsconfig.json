{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"sourceMap": false,
		"rootDir": "src",
		"strict": true,
		"skipLibCheck": true,
		"removeComments": true,
		"importHelpers": true,
		"noEmitHelpers": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"allowUnusedLabels": false,
		"allowUnreachableCode": false
	},
	"exclude": [
		"node_modules",
		".vscode-test",
		"test",
		"out"
	]
}
